{
	"main": "http://localhost:3000",
	"node-remote": "http://localhost:3000",
	"name": "Vite & NW.JS",
	"chromium-args": "--enable-webvr --webxr --enable-gamepad-extensions --enable-gamepad-vibration --enable-spell-checking --remote-debugging-port=9222",
	"description": "Vite & NW.JS",
